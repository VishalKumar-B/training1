<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.motivity.Student1POJO" table="student1">
		<id name="student_id" column="sid"></id>
		<property name="student_name" column="sname"></property>
		<many-to-one name="fuid" cascade="all" class="com.motivity.University1POJO" ></many-to-one>

	</class>
</hibernate-mapping>