<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.motivity.owner.QueryPOJO" table="query">

		<id name="query_id" column="query_id"></id>
		<property name="owner_id" column="owner_id"></property>
		<property name="owner_name" column="owner_name"></property>
		<property name="owner_email" column="owner_email"></property>
		<property name="owner_flat_number" column="owner_flat_number"></property>
		<property name="complaint" column="complaint"></property>
		<property name="status" column="status"></property>
	</class>
</hibernate-mapping>