<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.motivity.student.Student25POJO"
		table="Student25">

		<id name="student_id" column="id"></id>
		<property name="student_name" column="name"></property>
		<property name="student_section" column="section"></property>
		<property name="student_marks" column="marks"></property>
	</class>

</hibernate-mapping>